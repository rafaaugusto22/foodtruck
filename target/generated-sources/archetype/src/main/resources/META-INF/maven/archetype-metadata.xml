          <directory>src/test/java</directory>
          <includes>
            <include>**/*.java</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>test-output</directory>
          <includes>
            <include>**/*.xml</include>
            <include>**/*.html</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>test-output/Default suite</directory>
          <includes>
            <include>**/*.xml</include>
            <include>**/*.html</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>test-output/old</directory>
          <includes>
            <include>**/*.html</include>
            <include>**/*.properties</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>.settings</directory>
          <includes>
            <include>**/*.xml</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>test-output</directory>
          <includes>
            <include>**/*.css</include>
            <include>**/*.gif</include>
            <include>**/*.png</include>
            <include>**/*.js</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>.settings</directory>
          <includes>
            <include>**/*.container</include>
            <include>**/*.component</include>
            <include>**/*.public</include>
            <include>**/*.name</include>
            <include>**/*.jsdtscope</include>
            <include>**/*.prefs</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory></directory>
          <includes>
            <include>.classpath</include>
            <include>.project</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory></directory>
          <includes>
            <include>.dirty</include>
            <include>.gitignore</include>
            <include>.tern-project</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="${rootArtifactId}-ear" dir="ear" name="${rootArtifactId}-ear">
      <fileSets>
        <fileSet encoding="UTF-8">
          <directory>src/main/application</directory>
          <includes>
            <include>**/*.old</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>.settings</directory>
          <includes>
            <include>**/*.xml</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>.settings</directory>
          <includes>
            <include>**/*.component</include>
            <include>**/*.prefs</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory></directory>
          <includes>
            <include>.project</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory></directory>
          <includes>
            <include>.dirty</include>
            <include>.gitignore</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
  </modules>
</archetype-descriptor>
